{
	"name": "zh-new-logistics-platform",
	"version": "0.1.0",
	"license": "MIT",
	"description": "New logistics business platform for navinfo used by webpack4+vue2.5+vuex+Element UI+axios with permission and lint",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
		"mock": "cross-env NODE_ENV=mock webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
		"build": "cross-env NODE_ENV=prod node build/build.js",
		"build:testing": "cross-env NODE_ENV=testing node build/build.js",
		"build:mock": "cross-env NODE_ENV=mock node build/build.js",
		"build:report": "npm_config_report=true node build/build.js",
		"lint": "eslint --ext .js,.vue src",
		"fix": "eslint --fix --ext .js,.vue src",
		"e2e-test": "node ./tests/bin/runner.js --chrome",
		"selenium-setup": "node ./tests/bin/selenium-setup.js",
		"selenium-start": "node ./tests/bin/selenium-start.js",
		"dev-init-scripts": "cross-env SCRIPT=y node ./server/init/index.js",
		"dev-clear-scripts": "node ./server/init/index.js",
		"server": "cross-env PORT=3010 nodemon ./server/app.js",
		"start": "npm run server & npm run dev"
	},
